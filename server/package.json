{
  "name": "simple-eam-server",
  "version": "1.0.4",
  "description": "Simple EAM GraphQL Server with Neo4j Database",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath && process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('⚠️ Please use yarn for this project!')\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "init-db": "ts-node src/db/init-db.ts",
    "init-db-pv": "ts-node src/db/pv/init-db-pv-runner.ts",
    "init-db-heatpump": "ts-node src/db/heatpump/init-db-heatpump-runner.ts",
    "test": "jest"
  },
  "packageManager": "yarn@4.9.1",
  "dependencies": {
    "@neo4j/graphql": "7.1.1",
    "apollo-server-core": "^3.12.1",
    "apollo-server-express": "^3.12.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "graphql-tools": "^9.0.18",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-client": "^2.0.5",
    "neo4j-driver": "^5.13.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.2",
    "@types/uuid": "^9.0.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
